{
  "name": "whatsapp-bot (AQUA-BOT)",  
  "description": "Whatsapp bot.",
  "repository": "https://github.com/Morrow37/Bot_morro",
  "stack": "container", 
  "env": {
    "DATABASE_URL": { 
      "required": false 
    },
    "SESSION_ID": { 
      "required": true 
    },
    "MODE": { 
      "required": true 
    },
    "PREFIX": {  
      "required": false
    },
    "HKEY": { 
      "required": true
    },
    "HAPP": { 
      "required": true,
      "value": "bottest" // Your Heroku app name added here
    },
    "statusview": {  
      "required": false 
    },
    "autoRead": {  
      "required": false 
    },
    "antidelete": {  
      "required": false 
    },
    "REMOVEBG_KEY": { 
      "required": false
    }
  },
  "buildpacks": [
    { "url": "heroku/nodejs" },
    { "url": "https://github.com/DuckyTeam/heroku-buildpack-imagemagick.git" },
    { "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest" },
    { "url": "https://github.com/clhuang/heroku-buildpack-webp-binaries.git" }
  ]
}
